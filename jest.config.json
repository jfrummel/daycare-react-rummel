{
    "setupFiles": [
        "raf/polyfill",
        "<rootDir>/src/tests/setupTests.js"
    ],
    "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>src/tests/setupTests.js"
    ],
    "bail": 1,
    "verbose": true,
    "transform": {
        "^.+\\.jsx?$": "babel-jest"
    }
}